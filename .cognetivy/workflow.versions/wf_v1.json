{
  "workflow_id": "wf_default",
  "version": "v1",
  "nodes": [
    {
      "id": "retrieve",
      "type": "TASK",
      "contract": {
        "input": [
          "topic"
        ],
        "output": [
          "sources"
        ]
      },
      "description": "Retrieves relevant sources for the given topic."
    },
    {
      "id": "synthesize",
      "type": "TASK",
      "contract": {
        "input": [
          "sources"
        ],
        "output": [
          "summary"
        ]
      },
      "description": "Synthesizes the retrieved sources into a summary."
    }
  ],
  "edges": [
    {
      "from": "retrieve",
      "to": "synthesize"
    }
  ]
}