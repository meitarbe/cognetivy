{
  "workflow_id": "wf_default",
  "version": "v3",
  "nodes": [
    {
      "id": "theme_scan",
      "type": "TASK",
      "contract": {
        "input": [
          "goal",
          "scope"
        ],
        "output": [
          "themes"
        ]
      }
    },
    {
      "id": "signal_search",
      "type": "TASK",
      "contract": {
        "input": [
          "themes"
        ],
        "output": [
          "signals"
        ]
      }
    },
    {
      "id": "synthesize",
      "type": "TASK",
      "contract": {
        "input": [
          "signals"
        ],
        "output": [
          "ideas"
        ]
      }
    },
    {
      "id": "evaluate",
      "type": "TASK",
      "contract": {
        "input": [
          "ideas"
        ],
        "output": [
          "scored_ideas"
        ]
      }
    },
    {
      "id": "reflect",
      "type": "TASK",
      "contract": {
        "input": [
          "scored_ideas",
          "run_artifacts"
        ],
        "output": [
          "learnings",
          "workflow_suggestions"
        ]
      }
    },
    {
      "id": "why_now_validate",
      "type": "TASK",
      "contract": {
        "input": [
          "ideas"
        ],
        "output": [
          "validated_ideas"
        ]
      }
    }
  ],
  "edges": [
    {
      "from": "theme_scan",
      "to": "signal_search"
    },
    {
      "from": "signal_search",
      "to": "synthesize"
    },
    {
      "from": "synthesize",
      "to": "why_now_validate"
    },
    {
      "from": "evaluate",
      "to": "reflect"
    },
    {
      "from": "why_now_validate",
      "to": "evaluate"
    }
  ]
}